package audience

import (
	"github.com/jfkgustav/direq/model"
	"github.com/jfkgustav/direq/view"
)

templ Index(songs []*model.Song) {
	@view.Layout() {
		<div class="px-2">
			for _, song := range songs {
				<div class="flex flex-col space-y-4">
					<div class="container bg-gray-100 px-2 py-2">
						@SongCard(song)
					</div>
					<br/>
				</div>
			}
		</div>
	}
}
